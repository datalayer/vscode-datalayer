{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "dom.iterable", "es2019"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["webview"],
  "exclude": ["node_modules", "src"]
}
