+    /** The completion text to display as ghost text */
+    completionText: string;
+}
+/**
+ * Serialized representation of InlineCompletionNode
+ */
+export type SerializedInlineCompletionNode = Spread<{
+    completionText: string;
+}, SerializedLexicalNode>;
