+}
+
+/* ============================================
+   Placeholder
+   ============================================ */
+
+.lexical-theme-placeholder {
+  color: var(--jp-ui-font-color3, var(--fgColor-muted));
+  overflow: hidden;
+  position: absolute;
+  text-overflow: ellipsis;
+  top: 16px;
+  left: 16px;
+  font-size: var(--jp-ui-font-size1, 16px);
+  user-select: none;
+  pointer-events: none;
+}
