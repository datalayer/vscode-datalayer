+                    resultSpaces = customRule.apply(resultSpaces, context);
+                    if (this.debug) {
+                        console.log(`[AutoIndentEngine] Custom rule applied: ${customRule.name}`);
+                    }
+                    break; // Use first matching custom rule
+                }
+            }
+        }
+        // Ensure we never go negative
